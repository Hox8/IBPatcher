<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PublishAot>true</PublishAot>
		<ProduceReferenceAssembly>False</ProduceReferenceAssembly>
		<ApplicationIcon>Resources\Hammer256_Indexed.ico</ApplicationIcon>
		<ApplicationManifest>app.manifest</ApplicationManifest>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<DebugType>full</DebugType>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<DebugType>none</DebugType>
	</PropertyGroup>

	<ItemGroup>
		<Content Include="Resources\Hammer256_Indexed.ico" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="System.Text.Json" Version="8.0.0-preview.3.23174.8" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\UnrealLib\UnrealLib.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Reference Include="Zip Reduced">
			<HintPath>Zip Reduced.dll</HintPath>
		</Reference>
	</ItemGroup>

</Project>